@model CreateFooterAddressDto
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="card container">
    <div class="card-body">

        <h4 class="card-title">Alt Adres Ekle</h4>
        <form method="post">

            <div class="form-group">
                <label asp-for="description">Açıklama</label>
                <input asp-for="description" class="form-control">
            </div>
           
            <div class="form-group">
                <label asp-for="address">Adres</label>
                <input asp-for="address" class="form-control">
            </div>
           
            <div class="form-group">
                <label asp-for="phone">Telefon</label>
                <input asp-for="phone" class="form-control">
            </div>
           
            <div class="form-group">
                <label asp-for="email">Mail</label>
                <input asp-for="email" class="form-control">
            </div>
           
           

        

            <button type="submit" class="btn btn-success waves-effect waves-light">Kaydet</button>
        </form>

    </div>
</div>


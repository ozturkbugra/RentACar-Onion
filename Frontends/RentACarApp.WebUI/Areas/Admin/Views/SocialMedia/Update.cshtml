@model ResultSocialMediaDto
@{
    ViewData["Title"] = "Update";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}



<div class="card container">
    <div class="card-body">

        <h4 class="card-title">Sosyal Medya Güncelle</h4>
        <form method="post">

               <input type="hidden" asp-for="socialMediaID"/>
                
           
            <div class="form-group">
                <label asp-for="name">Adı</label>
                <input asp-for="name" class="form-control">
            </div>

            <div class="form-group">
                <label asp-for="url">URL</label>
                <input asp-for="url" class="form-control">
            </div>


            <div class="form-group">
                <label asp-for="icon">İkon</label>
                <input asp-for="icon" class="form-control">
            </div>


           



            <button type="submit" class="btn btn-warning waves-effect waves-light">Güncelle</button>
        </form>

    </div>
</div>

